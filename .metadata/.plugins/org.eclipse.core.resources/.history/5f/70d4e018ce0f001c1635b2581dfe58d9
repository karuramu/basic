package anonymous;

public class A {
	class D {
		int o = 25;
	}

	/*
	 * class without any name is know as anonymous class
	 */
	public static void main(String[] args) {
		A a = new A();
		D a1 = a.new D() {

		};
		C a2 = new C() {
			public void test() {
				System.out.println("from test");
				// System.out.println(i);
			}

		};
		a2.test();
		System.out.println(a1.o);

	}
}
