package collectionsexp;
//Collections in java is class  that help us to perform sorting search on collections
import java.util.ArrayList;
import java.util.Collections;

public class A {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		ArrayList a=new ArrayList();
		a.add(45);
		a.add(55);
		a.add(10);
		Collections.sort(a);
		System.out.println(a);
	}
// why can't we able to sort the hertogenous data for ex 10,44,10.5 in sort
}
